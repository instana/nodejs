{
  "name": "instana-nodejs-sensor",
  "version": "1.140.0",
  "description": "The Instana Node.js sensor (legacy adapter for previous package name)",
  "author": {
    "name": "Bastian Krol",
    "email": "bastian.krol@instana.com"
  },
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/legacy-sensor/README.md",
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src",
    "CHANGELOG.md"
  ],
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "scripts": {
    "audit": "bin/prepare-audit.sh && npm audit --production; AUDIT_RESULT=$?; git checkout package-lock.json; exit $AUDIT_RESULT",
    "test": "mocha --sort $(find test -iname '*test.js' -not -path '*node_modules*')",
    "test:debug": "WITH_STDOUT=true npm run test",
    "test:ci": "echo \"******* Files to be tested:\n $CI_LEGACY_SENSOR_TEST_FILES\" && if [ -z \"${CI_LEGACY_SENSOR_TEST_FILES}\" ]; then echo \"No Files to test in this node\"; else mocha --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --sort ${CI_LEGACY_SENSOR_TEST_FILES}; fi",
    "lint": "eslint src test",
    "verify": "npm run lint && npm test",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js'"
  },
  "keywords": [
    "instana",
    "agent",
    "sensor",
    "monitoring",
    "tracing"
  ],
  "contributors": [
    {
      "name": "Bastian Krol",
      "email": "bastian.krol@instana.com"
    },
    {
      "name": "Willian Carvalho",
      "email": "willian.carvalho@instana.com"
    },
    {
      "name": "Katharina Irrgang",
      "email": "katharina.irrgang@instana.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/instana/nodejs/issues"
  },
  "dependencies": {
    "@instana/collector": "1.140.0"
  },
  "devDependencies": {
    "@instana/core": "1.140.0",
    "no-code2": "2.0.0"
  }
}

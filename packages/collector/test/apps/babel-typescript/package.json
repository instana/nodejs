{
  "name": "babel-example-app-with-instana",
  "version": "1.0.0",
  "description": "An example (and a test) for how to integrate @instana/collector into a babel app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "license": "MIT",
  "main": "lib/index",
  "scripts": {
    "build": "pnpm run build:types && pnpm run build:js",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "pnpm run type-check -- --watch"
  },
  "dependencies": {
    "@instana/collector": "latest"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-numeric-separator": "^7.14.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.2",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3"
  }
}

{
  "name": "@instana/collector",
  "version": "1.137.0",
  "description": "The Instana Node.js metrics and trace data collector",
  "author": {
    "name": "Ben Ripkens",
    "email": "ben@instana.com"
  },
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/collector/README.md",
  "license": "MIT",
  "main": "src/index.js",
  "bin": {
    "instana-instrument-edgemicro-cli": "src/bin/instrument-edgemicro-cli.js"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "dummy-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "scripts": {
    "audit": "bin/prepare-audit.sh && npm audit --production; AUDIT_RESULT=$?; git checkout package-lock.json; exit $AUDIT_RESULT",
    "test": "NODE_ENV=debug USE_OPENTRACING_DEBUG_IMPL=true npm run test:mocha",
    "test:debug": "WITH_STDOUT=true npm run test:mocha",
    "test:mocha": "echo \"******* Files to be tested:\n $CI_COLLECTOR_TEST_FILES\" && mocha --sort --config=test/.mocharc.json --reporter mocha-multi --reporter-options spec=-,xunit=../../test-results/collector/results.xml ${CI_COLLECTOR_TEST_FILES:=$(find test -iname '*test.js' -not -path '*node_modules*')} test/globalHooks_test.js",
    "lint": "eslint src test dummy-app",
    "verify": "npm run lint && npm test",
    "agent-stub": "AGENT_PORT=3210 node test/apps/agentStub.js",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js' 'dummy-app/**/*.js'"
  },
  "keywords": [
    "instana",
    "agent",
    "collector",
    "sensor",
    "monitoring",
    "tracing"
  ],

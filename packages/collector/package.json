{
  "name": "@instana/collector",
  "version": "3.14.4",
  "description": "The Instana Node.js metrics and trace data collector",
  "keywords": [
    "instana",
    "agent",
    "collector",
    "monitoring",
    "tracing"
  ],
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/collector/README.md",
  "bugs": {
    "url": "https://github.com/instana/nodejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "license": "MIT",
  "author": {
    "name": "Ben Ripkens",
    "email": "ben@instana.com"
  },
  "contributors": [
    {
      "name": "Willian Carvalho",
      "email": "willian.carvalho@instana.com"
    },
    {
      "name": "Katharina Irrgang",
      "email": "katharina.irrgang@ibm.com"
    },
    {
      "name": "Arya Mohanan",
      "email": "arya.mohanan@ibm.com"
    }
  ],
  "main": "src/index.js",
  "bin": {
    "instana-instrument-edgemicro-cli": "src/bin/instrument-edgemicro-cli.js"
  },
  "directories": {
    "lib": "src",
    "example": "dummy-app",
    "test": "test"
  },
  "files": [
    "src",
    "CHANGELOG.md",
    "esm-loader.mjs",
    "esm-register.mjs"
  ],
  "scripts": {
    "agent-stub": "AGENT_PORT=3210 node test/apps/agentStub.js",
    "audit": "pnpm audit --prod",
    "lint": "eslint src test dummy-app",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js' 'dummy-app/**/*.js'",
    "test": "NODE_ENV=debug USE_OPENTRACING_DEBUG_IMPL=true mocha --config=test/.mocharc.js --require test/hooks.js --sort $(find test -iname '*test.js')",
    "test:ci:general": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/**/*test.js' --exclude 'test/tracing/**/*test.js'",
    "test:ci:tracing:cloud:aws:v2": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/cloud/aws-sdk/v2/**/*test.js'",
    "test:ci:tracing:cloud:aws:v3": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/cloud/aws-sdk/v3/**/*test.js'",
    "test:ci:tracing:cloud:azure": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/cloud/azure/**/*test.js'",
    "test:ci:tracing:cloud:gcp": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/cloud/gcp/**/*test.js'",
    "test:ci:tracing:database": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/database/**/*test.js'",
    "test:ci:tracing:frameworks": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/frameworks/**/*test.js'",
    "test:ci:tracing:general": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/**/*test.js' --exclude 'test/tracing/misc/**/*test.js' --exclude 'test/tracing/logger/**/*test.js' --exclude 'test/tracing/database/**/*test.js' --exclude 'test/tracing/frameworks/**/*test.js' --exclude 'test/tracing/cloud/**/*test.js' --exclude 'test/tracing/protocols/**/*test.js' --exclude 'test/tracing/messaging/**/*test.js'",
    "test:ci:tracing:logger": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/logger/**/*test.js'",
    "test:ci:tracing:messaging": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/messaging/**/*test.js'",
    "test:ci:tracing:misc": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/misc/**/*test.js'",
    "test:ci:tracing:protocols": "mocha --config=test/.mocharc.js --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --require test/hooks.js 'test/tracing/protocols/**/*test.js'",
    "test:debug": "WITH_STDOUT=true pnpm run test",
    "verify": "pnpm run lint && pnpm test"
  },
  "dependencies": {
    "@instana/core": "workspace:../core",
    "@instana/shared-metrics": "workspace:../shared-metrics",
    "bunyan": "^1.8.15",
    "semver": "^7.5.4",
    "serialize-error": "^8.1.0"
  },
  "optionalDependencies": {
    "@instana/autoprofile": "workspace:../autoprofile"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

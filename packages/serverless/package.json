{
  "name": "@instana/serverless",
  "version": "1.137.2",
  "description": "Internal utility package for serverless Node.js tracing and monitoring with Instana",
  "author": {
    "name": "Bastian Krol",
    "email": "bastian.krol@instana.com"
  },
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/serverless/README.md",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "scripts": {
    "audit": "bin/prepare-audit.sh && npm audit --production; AUDIT_RESULT=$?; git checkout package-lock.json; exit $AUDIT_RESULT",
    "test": "npm run test:mocha",
    "test:mocha": "echo \"******* Files to be tested:\n $CI_SERVERLESS_TEST_FILES\" && mocha --sort --reporter mocha-multi --reporter-options spec=-,xunit=../../test-results/serverless/results.xml ${CI_SERVERLESS_TEST_FILES:=$(find test -iname '*test.js' -not -path '*node_modules*')}",
    "test:debug": "WITH_STDOUT=true npm run test:mocha",
    "lint": "eslint src test",
    "verify": "npm run lint && npm test",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js'"
  },
  "keywords": [
    "distributed-tracing",
    "lambda",
    "faas",
    "instana",
    "memory",
    "metrics",
    "monitoring",
    "opentracing",
    "performance",
    "sensor",
    "serverless",
    "tracing"
  ],
  "contributors": [
    {
      "name": "Bastian Krol",
      "email": "bastian.krol@instana.com"
    },
    {
      "name": "Willian Carvalho",
      "email": "willian.carvalho@instana.com"
    },
    {
      "name": "Katharina Irrgang",
      "email": "katharina.irrgang@instana.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/instana/nodejs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@instana/core": "1.137.2",
    "agent-base": "^6.0.2",
    "https-proxy-agent": "^5.0.0",
    "semver": "7.3.3"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "mocha": "^7.2.0",
    "pino": "^6.11.3",
    "prettier": "^2.3.2"
  }
}

{
  "name": "@instana/aws-lambda",
  "version": "1.133.0",
  "description": "Instana tracing and monitoring for Node.js based AWS Lambdas",
  "author": {
    "name": "Bastian Krol",
    "email": "bastian.krol@instana.com"
  },
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/aws-lambda/README.md",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "example": "lambdas"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "scripts": {
    "audit": "bin/prepare-audit.sh && npm audit --production; AUDIT_RESULT=$?; git checkout package-lock.json; exit $AUDIT_RESULT",
    "test": "echo \"$(node -v)\" | grep v8 > /dev/null || npm run test:mocha",
    "test:mocha": "mocha --sort --reporter mocha-multi --reporter-options spec=-,xunit=../../test-results/aws-lambda/results.xml $(find test -iname '*test.js' -not -path '*node_modules*')",
    "test:debug": "WITH_STDOUT=true npm run test:mocha",
    "lint": "eslint src test lambdas",
    "verify": "npm run lint && npm test",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js' 'lambdas/**/*.js'"
  },
  "keywords": [
    "distributed-tracing",
    "aws",
    "lambda",
    "faas",
    "instana",
    "memory",
    "metrics",
    "monitoring",
    "opentracing",
    "performance",
    "sensor",
    "tracing"
  ],
  "contributors": [
    {
      "name": "Bastian Krol",
      "email": "bastian.krol@instana.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/instana/nodejs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@instana/core": "1.133.0",
    "@instana/serverless": "1.133.0"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha": "^8.1.0",
    "express": "^4.17.1",
    "mocha": "^7.2.0",
    "pino": "^6.11.3",
    "prettier": "^2.3.2"
  }
}

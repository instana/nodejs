{
  "name": "@instana/aws-fargate",
  "version": "1.140.1",
  "description": "Instana tracing and monitoring for Node.js based AWS Fargate tasks",
  "author": {
    "name": "Bastian Krol",
    "email": "bastian.krol@instana.com"
  },
  "homepage": "https://github.com/instana/nodejs/blob/main/packages/aws-fargate/README.md",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instana/nodejs.git"
  },
  "scripts": {
    "audit": "bin/prepare-audit.sh && npm audit --production; AUDIT_RESULT=$?; git checkout package-lock.json; exit $AUDIT_RESULT",
    "test": "mocha --sort $(find test -iname '*test.js' -not -path '*node_modules*')",
    "test:debug": "WITH_STDOUT=true npm run test",
    "test:ci": "echo \"******* Files to be tested:\n $CI_AWS_FARGATE_TEST_FILES\" && if [ -z \"${CI_AWS_FARGATE_TEST_FILES}\" ]; then echo \"No Files to test in this node\"; else mocha --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json --sort ${CI_AWS_FARGATE_TEST_FILES}; fi",
    "lint": "eslint src test images",
    "verify": "npm run lint && npm test",
    "prettier": "prettier --write 'src/**/*.js' 'test/**/*.js' 'images/**/*.js'"
  },
  "keywords": [
    "distributed-tracing",
    "aws",
    "fargate",
    "caas",
    "instana",
    "memory",
    "metrics",
    "monitoring",
    "opentracing",
    "performance",
    "sensor",
    "tracing"
  ],
  "contributors": [
    {
      "name": "Bastian Krol",
      "email": "bastian.krol@instana.com"
    },
    {
      "name": "Willian Carvalho",
      "email": "willian.carvalho@instana.com"
    },
    {
      "name": "Katharina Irrgang",
      "email": "katharina.irrgang@instana.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/instana/nodejs/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@instana/core": "1.140.1",
    "@instana/metrics-util": "1.140.1",
    "@instana/serverless": "1.140.1"
  },
  "devDependencies": {
    "no-code2": "2.0.0"
  },
  "engines": {
    "node": ">=10.4.0"
  }
}

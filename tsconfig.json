{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "allowJs": true,
    "checkJs": true,
    "lib": ["esnext.asynciterable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": true,
    "noImplicitAny": true,
    "maxNodeModuleJsDepth": 0
  },
  "include": [
    "packages/core/src/util",
    "packages/core/src/tracing/*.js",
  ],
  "exclude": [
    "node_modules",
  ]
}
